{
  "name": "minimax-router",
  "display_name": "MiniMax (路由器-思考模式)",
  "provider": "minimax",
  "description": "通过本地路由器服务启用思考能力的MiniMax M2系列模型",
  "requires_router": true,
  "thinking_enabled": true,
  "router_config": {
    "provider": "minimax",
    "model": "minimax-m2-thinking",
    "thinking_mode": true,
    "base_endpoint": "https://api.minimaxi.com/anthropic"
  },
  "env": {
    "ANTHROPIC_AUTH_TOKEN": "在此处输入您的MiniMax API密钥",
    "ANTHROPIC_BASE_URL": "http://localhost:3456/v1",
    "API_TIMEOUT_MS": "6000000",
    "ANTHROPIC_DEFAULT_OPUS_MODEL": "minimax-m2-thinking",
    "ANTHROPIC_DEFAULT_SONNET_MODEL": "minimax-m2-thinking",
    "ANTHROPIC_DEFAULT_HAIKU_MODEL": "minimax-m1.5"
  },
  "models": {
    "minimax-m2-thinking": "思考模式 - 高性能推理",
    "minimax-m1.5": "标准模型 - 通用对话",
    "minimax-m1-light": "轻量模型 - 快速响应"
  },
  "default_model": "minimax-m2-thinking",
  "metadata": {
    "created_at": "2024-01-01T00:00:00Z",
    "updated_at": "2024-01-01T00:00:00Z",
    "version": "1.0",
    "note": "需要运行 claude-code-router 服务"
  }
}
